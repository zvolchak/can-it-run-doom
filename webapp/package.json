{
  "name": "can-it-run-doom",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "18"
  },
  "main": "ssr.js",
  "scripts": {
    "env:staging": "doppler secrets download --project canitrundoom-webapp --config stg --no-file --format env > .env",
    "env:prod": "doppler secrets download --project canitrundoom-webapp --config prd --no-file --format env > .env",
    "dev": "doppler run -- next dev --turbopack",
    "build:prod": "npm run env:prod && ./scripts/build_prod.sh",
    "build:staging": "npm run env:staging && ./scripts/build_prod.sh",
    "build": "next build",
    "start": "next start",
    "start:dev": "nodemon",
    "lint": "next lint",
    "build:sitemap": "next-sitemap"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "firebase": "^11.2.0",
    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.2.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "react-redux": "^9.2.0",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "next-sitemap": "^4.2.3",
    "nodemon": "^3.1.9",
    "postcss": "^8",
    "sass": "^1.83.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
